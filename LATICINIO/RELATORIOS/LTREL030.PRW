*---------------------------------------------------------------------------*
Static Function ReportPrd()          
*---------------------------------------------------------------------------*
Local cAliasTMP		:= GetNextAlias()
Local hEnter		:= CHR(10) + CHR(13) 
Local CabecAux 		:= ""
Local x := VAL(mv_par02)

	
	If x > 0
		For nI := 1 to x
			nLin := 14
			@nLin,01 PSAY "                                                   05.341.357/0001-57"
			nLin +=3
			@nLin,01 PSAY "  TRES BARRAS IND. DE LACTEOS DO BRASIL LTDA             902.69501-01"
			nLin +=3
			@nLin,01 PSAY "  RUA JK, 1016 - JARDIM GUARANI"
			nLin +=2
			@nLin,01 PSAY "  TRES BARRAS DO PARANA         PR      85485-000      45 3235-8000"
			nLin +=1
			@nLin,01 PSAY ""
			nLin +=6
			@nLin,01 PSAY "  LEITE IN NATURA                        LT"
			PROW( 0 )
		Next nI
	EndIf	
	
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Finaliza a execucao do relatorio...                                 ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู

	SET DEVICE TO SCREEN
	
	//ฺฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฟ
	//ณ Se impressao em disco, chama o gerenciador de impressao...          ณ
	//ภฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤฤู
	
	If aReturn[5]==1
	   dbCommitAll()
	   SET PRINTER TO
	   OurSpool(wnrel)
	Endif
	
	MS_FLUSH()
	        
	
Return
     
  
