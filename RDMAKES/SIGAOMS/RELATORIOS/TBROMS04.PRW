TRCell():New(oSection2, "% Acordo" , "",, "@E 999.99", 6,, {|| POSICIONE("SA1",1,xFilial("SA1")+(cAliasTMP)->C6_CLI+(cAliasTMP)->C6_LOJA,"A1_X_DESCF")}, "Acordo")
TRCell():New(oSection2, "C6_QTDVEN", "SC6", "Quantidade","@E 99,999,999")
TRCell():New(oSection2, "C6_PRCVEN", "SC6", "Pre็o Un","@E 999.99",6) 
