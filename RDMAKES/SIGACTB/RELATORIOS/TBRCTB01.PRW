TRCell():New(oSection2, "F1_EMISSAO", "SF1", "Dt Emissใo",,15)
TRCell():New(oSection2, "F1_ESPECIE", "SF1", "Esp้cie",,8)
TRCell():New(oSection2, "F1_DOC", "SF1", "Documento",,18)
TRCell():New(oSection2, "F1_SERIE", "SF1", "Serie",,6)
TRCell():New(oSection2, "F1_FORNECE", "SF1", "Fornecedor",,18)
TRCell():New(oSection2, "F1_LOJA", "SF1", "Loja",,8)
TRCell():New(oSection2, "Razใo Social" , "",, "@!", 30,, {|| POSICIONE("SA2",1,xFilial("SA2")+(cAliasTMP)->F1_FORNECE+(cAliasTMP)->F1_LOJA,"A2_NOME")}, "Razใo Social")
TRCell():New(oSection2, "F1_VALBRUT", "SF1", "Valor",,16)


Return oReport


